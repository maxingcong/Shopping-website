                <div class="form-item">
                    <div class="form-label">联系邮箱：</div>
                    <div class="form-text">{{email}}</div>
                </div>
                <div class="form-item">
                    <div class="form-label">联系QQ：</div>
                    <div class="form-text">{{qq}}</div>
