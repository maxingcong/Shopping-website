                            <option value="{{item.k}}">{{item.v}}</option>
                            {{/if}}
                            {{/each}}
                        </select>
                    </div>
                </div>
                <div class="form-item">
                    <div class="form-label">
                        <label>作品图片：</label>
                    </div>
                    <div class="form-right">
                        <div class="upload-images clearfix">
