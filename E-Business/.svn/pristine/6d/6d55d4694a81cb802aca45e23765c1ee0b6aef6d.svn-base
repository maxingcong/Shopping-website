/*delivery-view*/
.delivery-view .show_row > td {
    background: #fffbf2;
    border-top: 1px solid #f7cb91;
}
.delivery-view .show_row td:first-child {
    border-left: 1px solid #f7cb91;
}
.delivery-view .show_row td:last-child {
    border-right: 1px solid #f7cb91;
    margin-right: 1px;
}
.delivery-view .show_table .info-block {
    padding-top: 0;
    border-top: 0;
}
.delivery-view .show_table .load {
    min-height: 70px;
    background-position: center 20px;
}
.no-data {
    padding: 20px 0;
}

/*form_vcode*/
.form_vcode {}
.form_vcode .vcode { width: 160px; margin-right: 10px; font-size: 24px; line-height: 32px; padding: 10px; border: 1px solid #e7e7e7; vertical-align: middle;}
.form_vcode .vcode:focus { padding: 9px; border: 2px solid #dadada;}
.form_vcode .img { width: 123px; height: 54px; margin-right: 10px; vertical-align: middle;}
.form_vcode .refresh { vertical-align: middle; display: inline-block; line-height: 18px;}

/*文件上传*/
.up-cont {
    position: relative;
    text-align: center;
    overflow: hidden;
    display: inline-block;
    vertical-align: middle;
    cursor: default;
}
.up-cont.btn-xs {
    width: 90px;
    height: 24px;
}
.up-cont .up-btn {
    width: 100%;
    height: 100%;
    display: inline-block;
}
.up-cont .up-doing, .up-cont .up-forbid {
    display: none;
}
.up-ing .up-doing, .up-dis .up-forbid {
    position: absolute;
    z-index: 5;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #afafaf;
    display: inline-block;
}
.upload-info {
    text-align: left;
}
.upload-info .file-size {
    display: none;
}
.upload-info .progress {
    width: 128px;
    font-size: 0;
    line-height: 0;
    overflow: hidden;
    height: 16px;
    background-color: #e7e7e7;
    display: inline-block;
    vertical-align: middle;
}
.upload-info .progress-bar {
    width: 0;
    height: 100%;
    font-size: 12px;
    line-height: 16px;
    color: #ff0000;
    text-align: right;
    background-color: #0487e2;
    display: inline-block;
}
.upload-info .file-cancel {
    color: #0487e2;
    line-height: 16px;
    font-weight: bold;
    margin: 0 0 0 5px;
    display: inline-block;
    vertical-align: middle;
    text-decoration: none;
}
.upload-info .file-cancel:hover {
    color: #036ad8;
}

@media (max-width: 768px) {
    .bs-header {
        position: static;
    }
    .bs-container {
        padding: 0 0 50px 0;
    }
    .bs-wrapper {
        margin: 0;
    }
    .bs-sidebar-affix {
        display: none;
    }
    .bs-sidebar {
        position: static;
        width: auto;
        height: auto;
        padding: 0;
        float: none;
    }
    .bs-sidebar > .nav {
        margin-bottom: 0;
    }
    .bs-navbar-fixed-top.affix-top {
        position: absolute;
        top: 0;
    }
    .bs-navbar-fixed-top.affix {
        top: 0;
    }
}
@media (max-width: 480px) {
    .bs-header .navbar-brand img:last-child {
        display: none;
    }
    .bs-header .navbar-nav {
        margin-top: 0;
    }
    .copyright span:first-child {
        display: none;
    }
}