/*bs-tree*/
.bs-tree {
    min-width: 230px;
    min-height: 640px;
}
.bs-tree .bs-tree-head {
    height: 40px;
    margin: 0;
    padding: 0 5px;
    background: #fbfbfb;
    border-bottom: 1px solid #ccd5df;
}
.bs-tree .bs-tree-head li {
    padding: 5px 5px;
}
.bs-tree .bs-tree-body {
    margin: 5px 10px 0;
    overflow: auto;
}
.bs-tree .bs-tree-body li {
    height: 31px;
    padding-left: 20px;
    white-space: nowrap;
    overflow: hidden;
}
.bs-tree .bs-tree-body li.open {
    height: auto;
}
.bs-tree .bs-tree-body a ~ .fa {
    font-size: 16px;
    margin: 0 3px;
    cursor: pointer;
}
.bs-tree .bs-tree-body li > .fa:first-child {
    font-size: 18px;
    margin-top: 2px;
    margin-left: -18px;
    vertical-align: middle;
    cursor: pointer;
    min-width: 14px;
    text-align: right;
}
.bs-tree .bs-tree-body li a {
    color: #555;
    line-height: 20px;
    height: 31px;
    padding: 5px 5px 5px 0;
    overflow: hidden;
    background-color: transparent;
    display: inline-block;
    vertical-align: middle;
}
.bs-tree .bs-tree-body li a:hover {
    color: #0093dd;
    background-color: transparent;
}
.bs-tree .bs-tree-body .active {
    position: relative;
    background-color: transparent;
}
.bs-tree .bs-tree-body .active:before {
    position: absolute;
    z-index: -1;
    top: 0;
    left: 0;
    width: 100%;
    height: 31px;
    content: "";
    background-color: #e5f4fc;
    display: block;
}
.bs-tree .bs-tree-body .open > a,
.bs-tree .bs-tree-body .open > a:hover,
.bs-tree .bs-tree-body .open > a:focus {
    color: #555;
    background-color: transparent;
}
.bs-tree .bs-tree-body .active > .fa,
.bs-tree .bs-tree-body .active > a,
.bs-tree .bs-tree-body .active > a:hover,
.bs-tree .bs-tree-body .active > a:focus {
    color: #0085f3;
    background-color: transparent;
}
.bs-tree .bs-tree-body .radio, .bs-tree .checkbox, .bs-tree .bs-tree-body .radio label, .bs-tree .checkbox label {
    margin: 0;
    display: inline-block;
}
