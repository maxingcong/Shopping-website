.paginbar a:hover { color: #fff; background-color: #0093dd; text-decoration: none}
.paginbar .dis { background: #ccc; cursor: default; }
.paginbar .go { width: 50px; }
.paginbar .btn { margin: 0; border-left: 0; font-size: 20px;}
