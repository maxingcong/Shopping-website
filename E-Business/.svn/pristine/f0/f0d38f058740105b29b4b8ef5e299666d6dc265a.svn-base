            var browser = {},
                matched = (function (ua) {
                    var match = /(chrome)[ \/]([\w.]+)/.exec(ua) ||
                        ///(webkit)[ \/]([\w.]+)/.exec( ua ) ||
                        ///version[\/][\d.]+.*(safari)[ \/]([\w.]+)/.exec(ua) ||
                        /version[\/]([\d.]+).*(safari)[ \/][\w.]+/.exec(ua) ||
