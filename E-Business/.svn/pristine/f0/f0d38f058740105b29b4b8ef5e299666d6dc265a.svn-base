                        ua.indexOf("compatible") < 0 && /(mozilla)(?:.*? rv:([\w.]+)|)/.exec(ua) || [];
                    if (match[2] === "safari") {
                        return {
                            browser: match[2] || "",
                            version: match[1] || "0"
                        };
                    }
                    return {
                        browser: match[1] || "",
                        version: match[2] || "0"
                    };
                })(userAgent.toLowerCase());
            if (matched.browser) {
                browser[matched.browser] = true;
                browser.version = matched.version;
            }
            return browser;
        }));
